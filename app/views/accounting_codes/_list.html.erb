	<table class="table table-bordered table-sm">
<thead>
	<tr>
		<th>Code</th>
		<th>Name</th>
		<th>Description</th>
		<th>Category</th>
		<th>Status</th>
		<th>&nbsp;</th>
	</tr>
</thead>
<tbody>
	<% @accounting_codes.each do |accounting_code| %>
		<tr>
			<td><%= accounting_code.code %></td>
			<td><%= accounting_code.account %></td>
			<td><%= accounting_code.description %></td>
			<td><%= accounting_code.category %></td>
			<td><%= accounting_code.status %></td>
			<td><%= link_to 'View', accounting_code_path(accounting_code.id) %></td>
			<td><%= link_to 'Delete', accounting_code_path(accounting_code.id), method: :delete, data: {confirm: "Are you sure?"}%></td>
		</tr>
	<% end %>
</tbody>
	</table>
